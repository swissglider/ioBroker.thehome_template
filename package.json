{
  "name": "iobroker.thehome_template",
  "version": "0.0.2-0",
  "description": "This is the Home Template for all the adapters from Swissglider",
  "author": {
    "name": "Swissglider",
    "email": "npm@mailschweiz.com"
  },
  "homepage": "https://github.com/swissglider/ioBroker.thehome_template",
  "license": "MIT",
  "keywords": [
    "template",
    "theHome",
    "Swissglider",
    "test",
    "not productive"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/swissglider/ioBroker.thehome_template"
  },
  "dependencies": {
    "@iobroker/adapter-core": "^2.5.1"
  },
  "devDependencies": {
    "@alcalzone/release-script": "^2.2.2",
    "@iobroker/testing": "^2.5.2",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "@types/chai": "^4.3.0",
    "@types/chai-as-promised": "^7.1.4",
    "@types/gulp": "^4.0.9",
    "@types/jest": "^26.0.24",
    "@types/mocha": "^9.1.0",
    "@types/node": "^14.18.9",
    "@types/proxyquire": "^1.3.28",
    "@types/react": "^16.14.21",
    "@types/react-dom": "^16.9.14",
    "@types/sinon": "^10.0.8",
    "@types/sinon-chai": "^3.2.8",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "axios": "^0.25.0",
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "gulp": "^4.0.2",
    "mocha": "^9.1.4",
    "prettier": "^2.5.1",
    "proxyquire": "^2.1.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "^5.0.0",
    "rimraf": "^3.0.2",
    "sinon": "^12.0.1",
    "sinon-chai": "^3.7.0",
    "source-map-support": "^0.5.21",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4",
    "web-vitals": "^0.2.4",
    "workbox-background-sync": "^5.1.4",
    "workbox-broadcast-update": "^5.1.4",
    "workbox-cacheable-response": "^5.1.4",
    "workbox-core": "^5.1.4",
    "workbox-expiration": "^5.1.4",
    "workbox-google-analytics": "^5.1.4",
    "workbox-navigation-preload": "^5.1.4",
    "workbox-precaching": "^5.1.4",
    "workbox-range-requests": "^5.1.4",
    "workbox-routing": "^5.1.4",
    "workbox-strategies": "^5.1.4",
    "workbox-streams": "^5.1.4"
  },
  "main": "build/main.js",
  "files": [
    "admin{,/!(src)/**}/!(tsconfig|tsconfig.*).json",
    "admin{,/!(src)/**}/*.{html,css,png,svg,jpg,js}",
    "build/",
    "www/",
    "io-package.json",
    "LICENSE"
  ],
  "scripts": {
    "prebuild": "rimraf ./build",
    "build:ts": "tsc -p tsconfig.build.json",
    "watch:ts": "tsc -p tsconfig.build.json --watch",
    "watch": "npm run watch:ts",
    "test:ts": "mocha --config test/mocharc.custom.json src/**/*.test.ts",
    "test:package": "mocha test/package --exit",
    "test:unit": "mocha test/unit --exit",
    "test:integration": "mocha test/integration --exit",
    "test": "npm run test:ts && npm run test:package",
    "check": "tsc --noEmit",
    "lint": "eslint --ext .ts src/",
    "release": "release-script",
    "build:startWebApp": "mkdir -p ./www && rm -rf ./reactClientApp/thehome/build &&  npm --prefix ./reactClientApp/thehome run build && cp -R ./reactClientApp/thehome/build/* ./www/",
    "build:startWebApp_github_actions": "npm --prefix ./reactClientApp/thehome run build && cp -R ./reactClientApp/thehome/build/* ./www/",
    "build": "npm run build:ts && npm run build:startWebApp",
    "build_github_actions": "npm run build:ts && npm run build:startWebApp_github_actions",
    "startWebApp": "cd ./reactClientApp/thehome && npm start"
  },
  "bugs": {
    "url": "https://github.com/swissglider/ioBroker.thehome_template/issues"
  },
  "readmeFilename": "README.md"
}
